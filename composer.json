{
    "name": "vanthao03596/ghtk-sdk",
    "description": "This is my package GhtkSdk",
    "keywords": [
        "vanthao03596",
        "ghtk-sdk"
    ],
    "homepage": "https://github.com/vanthao03596/ghtk-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "phamthao",
            "email": "phamthao03596@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Vanthao03596\\GhtkSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanthao03596\\GhtkSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
